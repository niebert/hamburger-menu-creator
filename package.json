{
  "name": "jsoneditor4menu",
  "exportvar": "JSONEditor4Menu",
  "version": "0.0.1",
  "description": "JSON Editor for creating HTML5 menus with CSS and HTML Templates for Hamburger Menus - based on JSON Editor of Jeremy Dorn",
  "main": "src/main.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "build4code": "^0.2.4",
    "concat-files": "^0.1.1",
    "doctoc": "^1.3.0",
    "jsdom": "^13.1.0",
    "uglify-js": "^3.6.0"
  },
  "scripts": {
    "test": "node ./src/main.js; jshint dist/jsoneditor4menu.js",
    "build": "node ./build.js; uglifyjs src/main.js > dist/jsoneditor4menu.min.js;doctoc ./README.md",
    "compress": "uglifyjs src/main.js > dist/jsoneditor4menu.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niebert/hamburger-menu-creator.git"
  },
  "build": {
    "html": "docs/index_build.html",
    "css": "docs/css/build.css",
    "readme": "README.md",
    "htmlsrc": "docs/index_src_libs_build.html"
  },
  "keywords": [
    "Hamburger Menu",
    "CSS",
    "Javascript",
    "Editor",
    "JSON",
    "Schema",
    "CSS",
    "Menu",
    "Generation"
  ],
  "author": "Engelbert Niehaus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/niebert/hamburger-menu-creator/issues"
  },
  "homepage": "https://niebert.github.io/hamburger-menu-creator",
  "dependencies": {
    "handlebars": "^4.0.11",
    "linkparam": "^1.0.8",
    "shelljs": "^0.8.3"
  }
}
